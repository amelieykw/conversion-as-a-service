<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>CollectData</servlet-name>
    <servlet-class>com.yukaiwen.Front.CollectData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CollectData</servlet-name>
    <url-pattern>/collectdata</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PushQueueHandler</servlet-name>
    <servlet-class>com.yukaiwen.Handler.PushQueueHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PushQueueHandler</servlet-name>
    <url-pattern>/handler/pushqueuehandler</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>